<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.camnter.savevolley.samples">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name="com.camnter.savevolley.samples.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.camnter.savevolley.samples.gson.HurlGsonActivity"/>
        <activity android:name="com.camnter.savevolley.samples.gson.Okhttp3GsonActivity"/>
        <activity android:name="com.camnter.savevolley.samples.gson.HurlAgeraGsonActivity"/>
        <activity android:name="com.camnter.savevolley.samples.gson.Okhttp3AgeraGsonActivity"/>
    </application>

</manifest>